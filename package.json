{
  "name": "@antv/larkmap",
  "version": "0.4.0",
  "description": "A React toolkit for geospatial visualization based on L7",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "dumi dev",
    "lint": "run-p lint:*",
    "lint:ts": "eslint src __tests__ --ext .ts,.tsx",
    "lint:style": "stylelint --fix src/**/*.less",
    "prettier": "prettier --write 'src/*.{tsx,ts,md,json}'",
    "lint-fix": "eslint src __tests__ --fix --ext .ts,.tsx --format=pretty && npm run lint:style",
    "test": "jest",
    "test-cover": "jest --coverage",
    "clean": "rimraf dist",
    "size": "limit-size",
    "build": "father-build",
    "docs:build": "dumi build",
    "docs:build-analyze": "ANALYZE=1 dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "ci": "run-s lint build",
    "prepublishOnly": "npm run ci",
    "version": "node scripts/sync-version"
  },
  "dependencies": {
    "@antv/event-emitter": "^0.1.3",
    "@antv/l7": "^2.9.32-alpha.4",
    "@antv/l7-composite-layers": "^0.7.1",
    "@antv/l7-draw": "^3.0.6",
    "@formily/antd": "^2.1.11",
    "@formily/core": "^2.1.11",
    "@formily/react": "^2.1.11",
    "@types/geojson": "^7946.0.8",
    "ahooks": "^3.3.13",
    "classnames": "^2.3.1",
    "lodash-es": "^4.17.21",
    "query-string": "^7.1.1",
    "react-color": "^2.19.3",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "geotiff": "1.0.0-beta.10",
    "@ant-design/icons": "^4.7.0",
    "@antv/dumi-theme-antv": "^0.2.2",
    "@antv/l7": "^2.9.32-alpha.4",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/jest": "^26.0.24",
    "@types/lodash-es": "^4.17.6",
    "@umijs/fabric": "^2.10.2",
    "antd": "^4.22.3",
    "dumi": "^1.1.40",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "father-build": "^1.22.1",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "jest-less-loader": "^0.1.2",
    "limit-size": "^0.1.4",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@antv/l7": "^2.9.32-alpha.4",
    "antd": "^4.22.3",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "lint-staged": {
    "*.less": "stylelint --fix",
    "*.{ts,tsx}": "eslint --fix",
    "*.{ts,tsx,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "limit-size": [
    {
      "path": "dist/larkmap.min.js",
      "limit": "100 Kb"
    }
  ],
  "keywords": [
    "antv",
    "l7",
    "lark map"
  ],
  "author": {
    "name": "AntV",
    "url": "https://antv.vision/"
  },
  "homepage": "https://larkmap.antv.vision",
  "repository": {
    "type": "git",
    "branch": "main",
    "url": "https://github.com/antvis/LarkMap"
  },
  "bugs": {
    "url": "https://github.com/antvis/LarkMap/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
